{
    "name": "marcobax/nova-status-field",
    "description": "A Laravel Nova field for displaying statuses for Nova 4 and 5",
    "keywords": [
        "laravel",
        "status",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "MarcoBax\\StatusField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcoBax\\StatusField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
